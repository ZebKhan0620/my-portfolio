{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "ignoreCommand": "bash vercel-ignore-build.sh",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_DISABLE_ESLINT": "1"
  }
} 